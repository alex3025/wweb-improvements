{
  "name": "WWeb Improvements",
  "description": "A new dark mode for Whats App Web among other tweaks.",
  "version": "2.0.4",
  "author": "alex3025",
  "content_scripts": [
    {
      "css": [
        "src/out/styles.css"
      ],
      "matches": [
        "https://web.whatsapp.com/"
      ]
    }
  ],
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png",
    "32": "images/32.png",
    "16": "images/16.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "src/assets/inter/*"
      ],
      "matches": [
        "https://web.whatsapp.com/*"
      ]
    }
  ],
  "manifest_version": 3
}
